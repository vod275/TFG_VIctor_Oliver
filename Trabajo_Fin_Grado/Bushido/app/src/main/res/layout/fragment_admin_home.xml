<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.admin.AdminHomeFragment">


    <ImageView
        android:id="@+id/backgroundImage"
        android:layout_width="412dp"
        android:layout_height="734dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:contentDescription="@string/precios"
        android:scaleType="centerCrop"
        android:src="@drawable/fondo2"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="-2dp" />


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/transparentContainer"
        android:layout_width="411dp"
        android:layout_height="734dp"
        android:background="#C4FFFFFF"
        tools:layout_editor_absoluteY="-1dp"
        tools:ignore="MissingConstraints">


        <TextView
            android:id="@+id/tvPreciosIndividual"
            android:layout_width="254dp"
            android:layout_height="72dp"
            android:fontFamily="@font/russoone_regular"
            android:text="@string/individuales"
            android:textColor="@color/AzulTexto"
            android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.738"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/tvAbonosMananasIndividual"
            android:layout_width="240dp"
            android:layout_height="93dp"
            android:hint="@string/abono_ma_anas"
            android:textColorHint="@color/AzulTexto"
            app:boxStrokeColor="@color/AzulTexto"
            app:cursorColor="@color/AzulTexto"
            app:endIconMode="clear_text"
            app:endIconTint="@color/AzulTexto"
            app:helperTextEnabled="true"
            app:helperTextTextColor="@color/AzulTexto"
            app:hintTextColor="@color/AzulTexto"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.497"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.056">

            <com.google.android.material.textfield.TextInputEditText
                android:layout_width="match_parent"
                android:layout_height="61dp"
                android:backgroundTint="@android:color/transparent"
                android:fontFamily="@font/russoone_regular"
                android:textColor="@color/AzulTexto"
                android:textSelectHandle="@drawable/cursor"
                android:textSelectHandleLeft="@drawable/cursor"
                android:textSelectHandleRight="@drawable/cursor" />

        </com.google.android.material.textfield.TextInputLayout>


        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/tvAbonosIndividual"
            android:layout_width="240dp"
            android:layout_height="93dp"
            android:hint="@string/abono"
            android:textColorHint="@color/AzulTexto"
            app:boxStrokeColor="@color/AzulTexto"
            app:cursorColor="@color/AzulTexto"
            app:endIconMode="clear_text"
            app:endIconTint="@color/AzulTexto"
            app:helperTextEnabled="true"
            app:helperTextTextColor="@color/AzulTexto"
            app:hintTextColor="@color/AzulTexto"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.497"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.176">

            <com.google.android.material.textfield.TextInputEditText
                android:layout_width="match_parent"
                android:layout_height="61dp"
                android:backgroundTint="@android:color/transparent"
                android:fontFamily="@font/russoone_regular"
                android:textColor="@color/AzulTexto"
                android:textSelectHandle="@drawable/cursor"
                android:textSelectHandleLeft="@drawable/cursor"
                android:textSelectHandleRight="@drawable/cursor" />

        </com.google.android.material.textfield.TextInputLayout>


        <TextView
            android:id="@+id/tvPreciosPareja"
            android:layout_width="214dp"
            android:layout_height="81dp"
            android:fontFamily="@font/russoone_regular"
            android:text="@string/pareja"
            android:textColor="@color/AzulTexto"
            android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.741"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.339" />

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/tvAbonoPareja"
            android:layout_width="240dp"
            android:layout_height="93dp"
            android:hint="@string/abono_pareja"
            android:textColorHint="@color/AzulTexto"
            app:boxStrokeColor="@color/AzulTexto"
            app:cursorColor="@color/AzulTexto"
            app:endIconMode="clear_text"
            app:endIconTint="@color/AzulTexto"
            app:helperTextEnabled="true"
            app:helperTextTextColor="@color/AzulTexto"
            app:hintTextColor="@color/AzulTexto"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.497"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.425">

            <com.google.android.material.textfield.TextInputEditText
                android:layout_width="match_parent"
                android:layout_height="61dp"
                android:backgroundTint="@android:color/transparent"
                android:fontFamily="@font/russoone_regular"
                android:textColor="@color/AzulTexto"
                android:textSelectHandle="@drawable/cursor"
                android:textSelectHandleLeft="@drawable/cursor"
                android:textSelectHandleRight="@drawable/cursor" />

        </com.google.android.material.textfield.TextInputLayout>

        <TextView
            android:id="@+id/tvPreciosFamilia"
            android:layout_width="219dp"
            android:layout_height="75dp"
            android:fontFamily="@font/russoone_regular"
            android:text="@string/familiar"
            android:textColor="@color/AzulTexto"
            android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.734"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.597" />

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/tvAbonoFamiliar"
            android:layout_width="382dp"
            android:layout_height="92dp"
            android:hint="@string/familiar_hasta_4_personas_ni_os_21_a_os"
            android:textColorHint="@color/AzulTexto"
            app:boxStrokeColor="@color/AzulTexto"
            app:cursorColor="@color/AzulTexto"
            app:endIconMode="clear_text"
            app:endIconTint="@color/AzulTexto"
            app:helperTextEnabled="true"
            app:helperTextTextColor="@color/AzulTexto"
            app:hintTextColor="@color/AzulTexto"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.551"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.699">

            <com.google.android.material.textfield.TextInputEditText
                android:layout_width="match_parent"
                android:layout_height="61dp"
                android:backgroundTint="@android:color/transparent"
                android:fontFamily="@font/russoone_regular"
                android:textColor="@color/AzulTexto"
                android:textSelectHandle="@drawable/cursor"
                android:textSelectHandleLeft="@drawable/cursor"
                android:textSelectHandleRight="@drawable/cursor" />

        </com.google.android.material.textfield.TextInputLayout>

        <Button
            android:id="@+id/btnPreciosSociosAceptar"
            style="?attr/materialButtonOutlinedStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/russoone_regular"
            android:text="@string/subir_precios_socios"
            android:textColor="@color/AzulTexto"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.787"
            app:strokeColor="@color/AzulTexto" />

        <Button
            android:id="@+id/btnSubirFotosHome"
            style="?attr/materialButtonOutlinedStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/russoone_regular"
            android:text="@string/subir_fotos_home"
            android:textColor="@color/AzulTexto"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.492"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.902"
            app:strokeColor="@color/AzulTexto" />


    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>